{
    "name": "Gara-Adblocker",
    "manifest_version": 2,
    "version": "1.0",
    "description": "Adblocker",
    "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
    "background": {
        "scripts": ["background.js"]
    },
    "icons": {
        "16": "./icons/Logo16.png",
        "48": "./icons/Logo48.png",
        "128": "./icons/Logo128.png"
    }
}